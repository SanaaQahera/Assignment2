    #include <iostream>
    using namespace std;
     
    int main() {
     
    	double b,h,area;
    	cout <<"Enter lenghth of base of triangle\n";
    	cin >>b;
    	cout <<"Enter height of trianglel\n";
    	cin >>h;
    	area = 0.5*b*h;
    	cout <<"Area of triangle is = " <<area;
     
    	return 0;
    }
